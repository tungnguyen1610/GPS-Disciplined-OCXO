  rotate = <0>;
  rgb;
  fps = <30>;
 };
};




&{/} {

 gpio-keys {
  compatible = "gpio-keys";
  autorepeat;

  pinctrl-names = "default";
  pinctrl-0 = <
   &P2_02_gpio_pu_pin
   &P2_04_gpio_pu_pin
   &P2_06_gpio_pu_pin
   &P2_08_gpio_pu_pin
   &P2_18_gpio_pu_pin
   &P2_20_gpio_pu_pin
   &P2_22_gpio_pu_pin
   &P2_24_gpio_pu_pin
   &P1_31_gpio_pu_pin
   &P1_35_gpio_pu_pin
  >;

  lp-left {
   label = "left";
   linux,code = <105>;
   gpios = <&gpio1 28 1>;
  };

  lp-right {
   label = "right";
   linux,code = <106>;
   gpios = <&gpio1 26 1>;
  };

  lp-up {
   label = "up";
   linux,code = <103>;
   gpios = <&gpio1 25 1>;
  };

  lp-down {
   label = "down";
   linux,code = <108>;
   gpios = <&gpio1 27 1>;
  };

  rp-left {
   label = "p";
   linux,code = <25>;
   gpios = <&gpio1 12 1>;
  };

  rp-right {
   label = "tab";
   linux,code = <15>;
   gpios = <&gpio2 0 1>;
  };

  rp-up {
   label = "esc";
   linux,code = <1>;
   gpios = <&gpio1 14 1>;
  };

  rp-down {
   label = "enter";
   linux,code = <28>;
   gpios = <&gpio1 15 1>;
  };

  select {
   label = "five";
   linux,code = <6>;
   gpios = <&gpio3 18 1>;
  };

  start {
   label = "one";
   linux,code = <2>;
   gpios = <&gpio2 24 1>;
  };
 };

 capeleds {
  compatible = "gpio-leds";

  pinctrl-names = "default";
  pinctrl-0 = <
   &P1_04_gpio_pin
   &P2_03_gpio_pin
  >;

  led@1 {
   label = "gamepup::left";
   gpios = <&gpio2 25 0>;
   default-state = "off";
  };

  led@2 {
   label = "gamepup::right";
   gpios = <&gpio0 23 0>;
   default-state = "off";
  };
 };
};




