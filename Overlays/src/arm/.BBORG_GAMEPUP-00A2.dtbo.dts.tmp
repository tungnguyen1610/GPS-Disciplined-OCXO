  compatible = "sitronix,st7735r";
  pinctrl-names = "default";
  pinctrl-0 = <
   &P1_06_spi_cs_pin
   &P1_08_spi_sclk_pin
   &P1_10_spi_pin
   &P1_12_spi_pin
   &P2_19_gpio_pin
   &P2_17_gpio_pin
   &P2_01_gpio_pin
