 };

 panel {
  status = "okay";
  compatible = "ti,tilcdc,panel";
  pinctrl-names = "default";
  pinctrl-0 = <&bb_lcd_lcd_pins>;
  backlight = <&backlight>;
# 186 "src/arm/BB-BONE-LCD4-01-00A1.dts"
  panel-info {
   ac-bias = <255>;
   ac-bias-intrpt = <0>;
   dma-burst-sz = <16>;
   bpp = <16>;
   fdd = <0x80>;
   sync-edge = <0>;
   sync-ctrl = <1>;
   raster-order = <0>;
   fifo-th = <0>;
  };
  display-timings {
   native-mode = <&timing0>;

   timing0: 480x272 {
    clock-frequency = <9200000>;
    hactive = <480>;
    vactive = <272>;
    hfront-porch = <8>;
    hback-porch = <47>;
    hsync-len = <41>;
    vback-porch = <2>;
    vfront-porch = <3>;
    vsync-len = <10>;
    hsync-active = <0>;
