&am33xx_pinmux {
 bb_lcd_led_pins: pinmux_bb_lcd_led_pins {
  pinctrl-single,pins = <
   ((((0x878)) & 0xffff) - (0x0800)) ((((1 << 5) | (1 << 3))) | (7))
  >;
 };

 bb_lcd_pwm_backlight_pins: pinmux_bb_lcd_pwm_backlight_pins {
  pinctrl-single,pins = <
   ((((0x848)) & 0xffff) - (0x0800)) ((0) | (6))
  >;
 };

