  >;
 };

 bb_i2c1_pins: pinmux_bb_i2c1_pins {
  pinctrl-single,pins = <
   ((((0x958)) & 0xffff) - (0x0800)) (((1 << 5) | (1 << 4)) | (1 << 6) | 2)
   ((((0x95C)) & 0xffff) - (0x0800)) (((1 << 5) | (1 << 4)) | (1 << 6) | 2)
  >;
 };

 ar1021_pins: pinmux_ar1021_pins {
  pinctrl-single,pins = <
   ((((0x980)) & 0xffff) - (0x0800)) ((((1 << 5) | (1 << 3))) | (7))
  >;
 };
};

