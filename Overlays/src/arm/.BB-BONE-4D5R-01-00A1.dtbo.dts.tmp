&am33xx_pinmux {
 bb_lcd_pwm_backlight_pins: pinmux_bb_lcd_pwm_backlight_pins {
  pinctrl-single,pins = <
   ((((0x848)) & 0xffff) - (0x0800)) ((((1 << 3))) | (6))
  >;
 };

 bb_lcd_lcd_pins: pinmux_bb_lcd_lcd_pins {
  pinctrl-single,pins = <

   ((((0x9a4)) & 0xffff) - (0x0800)) ((((1 << 3))) | (7))

