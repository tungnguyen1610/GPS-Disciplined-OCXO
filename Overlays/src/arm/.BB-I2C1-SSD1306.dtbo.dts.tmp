/dts-v1/;
/plugin/;

# 1 "include/dt-bindings/board/am335x-bbw-bbb-base.h" 1
# 16 "src/arm/BB-I2C1-SSD1306.dts" 2
# 1 "include/dt-bindings/pinctrl/am33xx.h" 1
