/ {



 fragment@0 {
  target-path="/";
  __overlay__ {

   chosen {
    overlays {
     BB-I2C1-SSD1306 = "Wed Jun 19 07:10:02 2024";
    };
   };
  };
 };




 fragment@1 {
  target = <&ocp>;
  __overlay__ {
   P9_17_pinmux { status = "disabled"; };
   P9_18_pinmux { status = "disabled"; };
  };
 };

 fragment@2 {
  target = <&am33xx_pinmux>;
  __overlay__ {
   bb_i2c1_pins: pinmux_bb_i2c1_pins {
    pinctrl-single,pins = <
     0x158 0x72
     0x15C 0x72
    >;
   };
  };
 };

 fragment@3 {
  target = <&i2c1>;
  __overlay__ {
   status = "okay";
   pinctrl-names = "default";
   pinctrl-0 = <&bb_i2c1_pins>;


   clock-frequency = <400000>;

   #address-cells = <1>;
   #size-cells = <0>;

   ssd1306: oled@3c {
    compatible = "solomon,ssd1306fb-i2c";
    reg = <0x3c>;
    solomon,width = <128>;
    solomon,height = <64>;
    solomon,com-invdir;
    solomon,page-offset = <0>;
   };
  };
 };
};
