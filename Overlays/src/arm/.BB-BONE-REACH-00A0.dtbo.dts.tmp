# 1 "src/arm/BB-BONE-REACH-00A0.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "src/arm/BB-BONE-REACH-00A0.dts"





/dts-v1/;
/plugin/;

/ {



 fragment@0 {
  target-path="/";
  __overlay__ {

   chosen {
    overlays {
     BB-BONE-REACH-00A0 = "Wed Jun 19 07:10:02 2024";
    };
   };
  };
 };




 fragment@1 {
  target = <&ocp>;
  __overlay__ {
   P8_07_pinmux { status = "disabled"; };
   P9_14_pinmux { status = "disabled"; };
   P9_15_pinmux { status = "disabled"; };
   P9_17_pinmux { status = "disabled"; };
   P9_21_pinmux { status = "disabled"; };
   P9_26_pinmux { status = "disabled"; };
   P9_27_pinmux { status = "disabled"; };
  };
 };

 fragment@2 {
  target = <&am33xx_pinmux>;
  __overlay__ {
   pruicss_stepper_pins_reach: pinmux_pruicss_stepper_pins_reach{
    pinctrl-single,pins = <
     0x090 0x07
     0x048 0x07
     0x040 0x07
     0x15C 0x07
     0x154 0x07
     0x1A4 0x07
    >;
   };

   stepper_fault_pins_reach: pinmux_stepper_fault_pins_reach {
    pinctrl-single,pins = <
     0x180 0x37
    >;
   };
  };
 };

 fragment@3 {
  target = <&pruss>;
  __overlay__{
   status = "okay";
   pinctrl-names = "default";
   pinctrl-0 = <&pruicss_stepper_pins_reach>;

   stepper_ext_3 {
    pin-names = "Reach:pru-step_ext_3", "Reach:pru-dir_ext_3";
    gpios = <&gpio2 2 0
         &gpio1 18 0>;
   };

   stepper_ext_4 {
    pin-names = "Reach:pru-step_ext_4", "Reach:pru-dir_ext_4";
    gpios = <&gpio1 16 0
         &gpio0 5 0>;
   };

   stepper_ext_5 {
    pin-names = "Reach:pru-step_ext_5", "Reach:pru-dir_ext_5";
    gpios = <&gpio0 3 0
         &gpio3 19 0>;
   };
  };
 };

 fragment@4 {
  target = <&ocp>;
  __overlay__ {

   gpio_keys {
   compatible = "gpio-keys";
   pinctrl-names = "default";
   pinctrl-0 = <&stepper_fault_pins_reach>;

   fault_reach {
    label = "Reach:Fault";
    debounce_interval = <50>;
    linux,code = <95>;
    gpios = <&gpio0 14 0x5>;
    gpio-key,wakeup;
    };
   };
  };
 };

 fragment@5 {
  target = <&tscadc>;
  __overlay__ {
   status = "okay";
   adc {
    ti,adc-channels = <0 1 2 3 4 5 6 7>;
   };
  };
 };
};
