            hactive = <480>;
            vactive = <272>;
            hfront-porch = <8>;
            hback-porch = <47>;
            hsync-len = <41>;
            vback-porch = <2>;
            vfront-porch = <3>;
            vsync-len = <10>;
            hsync-active = <0>;
            vsync-active = <0>;
            de-active = <1>;
            pixelclk-active = <0>;
            };
        };
    };
};
