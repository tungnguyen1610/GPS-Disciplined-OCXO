            ac-bias = <255>;
            ac-bias-intrpt = <0>;
            dma-burst-sz = <16>;
            bpp = <16>;
            fdd = <0x80>;
            sync-edge = <0>;
            sync-ctrl = <1>;
            raster-order = <0>;
            fifo-th = <0>;
        };
        display-timings {
